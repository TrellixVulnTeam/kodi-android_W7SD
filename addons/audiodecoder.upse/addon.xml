<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="audiodecoder.upse"
  version="2.0.1"
  name="PSF Audio Decoder"
  provider-name="spiff">
  <requires>
<import addon="kodi.binary.global.main" version="1.0.14"/>
<import addon="kodi.binary.global.filesystem" version="1.0.2"/>
<import addon="kodi.binary.instance.audiodecoder" version="2.0.0"/></requires>
  <extension
    point="kodi.audiodecoder"
    name="psf"
    extension=".psf|.minipsf"
    tags="true"
    library_android="libaudiodecoder.upse.so"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Portable Sound Format (PSF) Audio Decoder</summary>
    <description lang="en">Adds decoding support for Sony Playstation Sound Format files (.PSF/.MINIPSF).</description>
    <platform>android-armv7</platform>
  </extension>
</addon>
